{
  "name": "birdsinthesun/contao-theme-falke",
  "type": "contao-theme",
  "homepage": "https://github.com/birdsinthesun/contao-theme-falke",
  "description":"Das Falke Theme ist ein Theme für das Contao CMS, das auf modernen Layout-Standards basiert und eine strukturierte, redaktionsfreundliche Umsetzung bietet. Es ist dazu gedacht die Erweiterung Fly UX näher kennenzulernen. ",
  "keywords":["theme","fly_ux","contao","mini","WYSIWYG","Drag_Drop"],
  "version": "1.1.0",
  "license": "MIT",
  "prefer-stable": true,
  "authors": [
        {
            "name": "Monique Hahnefeld",
            "email": "hello@bits-design.de",
            "homepage":"https://birdsinthesun.de",
            "role":"Developer"
        }
    ],
  "require": {
    "contao/core-bundle": "5.5.*",
    "contao/calendar-bundle": "5.5.*",
    "contao/comments-bundle": "5.5.*",
    "contao/conflicts": "*@dev",
    "contao/faq-bundle": "5.5.*",
    "contao/manager-bundle": "5.5.*",
    "contao/news-bundle": "5.5.*",
    "contao/newsletter-bundle": "5.5.*",
     "twig/intl-extra":"3.20.*",
      "birdsinthesun/fly_ux":"1.1.*"
  },
  "extra": {
    "contao-component-dir": "assets"
  },
  "scripts": {
    "post-install-cmd": [
      "@php vendor/bin/contao-setup"
    ],
    "post-update-cmd": [
      "@php vendor/bin/contao-setup"
    ]
  }
}
